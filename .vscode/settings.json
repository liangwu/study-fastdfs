    "gitHistory.sideBySide": true,
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.roundedSelection": false,
    "editor.fontSize": 16,
    "editor.wordWrap": "on",

	"clang-format.style" : "file",
