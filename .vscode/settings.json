{
	"files.exclude":{
		"**/build":true,
		"vscode-bitbake-build":true,
	},
	"workbench.colorCustomizations": {
        "editor.foreground":"#66D9EF",
    },
    "editor.codeActionsOnSave": {
        

    },
    "window.zoomLevel": 0,
    "json.schemas": [
        
    ], //editor.foreground : controls plain text color(控制纯文本颜色)
